{
  "name": "env-services",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && node src/scripts/favicon.js",
    "serve": "vite preview",
    "test": "jest --watch"
  },
  "dependencies": {
    "@xstate/react": "^1.3.1",
    "fp-ts": "^2.9.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.2.0",
    "reactivue": "^0.3.3",
    "ts-pattern": "^3.0.6",
    "xstate": "^4.16.2",
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.1",
    "@swc/core": "^1.2.50",
    "@swc/jest": "^0.1.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.16",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "rollup-plugin-favicons": "^0.2.0",
    "typescript": "^4.1.2",
    "vite": "^2.0.5",
    "vite-plugin-html": "^2.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/scripts/setupTest.ts"
    ],
    "clearMocks": true,
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
